{   "multipart": [
        {   "when": { "north": false, "east": false, "west": false, "south": false },
            "apply": { "model": "block/glass_pane_no_connections" }
        },
        {   "when": { "north": true, "south": false, "east": false, "west": false },
            "apply": { "model": "block/glass_pane_top_end" }
        },
        {   "when": { "north": false, "south": false, "east": true, "west": false },
            "apply": { "model": "block/glass_pane_top_end", "y": 90 }
        },
        {   "when": { "north": false, "south": true, "east": false, "west": false },
            "apply": { "model": "block/glass_pane_top_end", "y": 180 }
        },
        {   "when": { "north": false, "south": false, "east": false, "west": true },
            "apply": { "model": "block/glass_pane_top_end", "y": 270 }
        },
        {   "when": { "north": true, "south": true, "east": false, "west": false },
            "apply": { "model": "block/glass_pane_top_straight" }
        },
        {   "when": { "north": false, "south": false, "east": true, "west": true },
            "apply": { "model": "block/glass_pane_top_straight", "y": 90 }
        },
        {   "when": { "north": false, "south": true, "east": true, "west": false },
            "apply": { "model": "block/glass_pane_top_corner" }
        },
        {   "when": { "north": false, "south": true, "east": false, "west": true },
            "apply": { "model": "block/glass_pane_top_corner", "y": 90 }
        },
        {   "when": { "north": true, "south": false, "east": false, "west": true },
            "apply": { "model": "block/glass_pane_top_corner", "y": 180 }
        },
        {   "when": { "north": true, "south": false, "east": true, "west": false },
            "apply": { "model": "block/glass_pane_top_corner", "y": 270 }
        },
        {   "when": { "north": false, "south": true, "east": true, "west": true },
            "apply": { "model": "block/glass_pane_top_three_way" }
        },
        {   "when": { "north": true, "south": true, "east": false, "west": true },
            "apply": { "model": "block/glass_pane_top_three_way", "y": 90 }
        },
        {   "when": { "north": true, "south": false, "east": true, "west": true },
            "apply": { "model": "block/glass_pane_top_three_way", "y": 180 }
        },
        {   "when": { "north": true, "south": true, "east": true, "west": false },
            "apply": { "model": "block/glass_pane_top_three_way", "y": 270 }
        },
        {   "when": { "north": true, "south": true, "east": true, "west": true },
            "apply": { "model": "block/glass_pane_top_four_way", "y": 270 }
        },
        {   "when": { "north": true },
            "apply": { "model": "block/yellow_stained_glass_pane_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "block/yellow_stained_glass_pane_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "block/yellow_stained_glass_pane_side_alt" }
        },
        {   "when": { "west": true },
            "apply": { "model": "block/yellow_stained_glass_pane_side_alt", "y": 90 }
        },
        {   "when": { "OR": [
                { "north": false, "west": true},
                { "north": false, "east": true },
                { "north": false, "south": true }
            ]},
            "apply": { "model": "block/yellow_stained_glass_pane_noside" }
        },
        {   "when": { "OR": [
                { "east": false, "north": true},
                { "east": false, "west": true },
                { "east": false, "south": true }
            ]},
            "apply": { "model": "block/yellow_stained_glass_pane_noside_alt" }
        },
        {   "when": { "OR": [
                { "south": false, "north": true},
                { "south": false, "east": true },
                { "south": false, "west": true }
            ]},
            "apply": { "model": "block/yellow_stained_glass_pane_noside_alt", "y": 90 }
        },
        {   "when": { "OR": [
                { "west": false, "north": true},
                { "west": false, "east": true },
                { "west": false, "south": true }
            ]},
            "apply": { "model": "block/yellow_stained_glass_pane_noside", "y": 270 }
        }
    ]
}